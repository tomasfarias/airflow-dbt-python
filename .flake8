[flake8]
max-line-length = 88
docstring-convention = google
ignore = D107, W503
per-file-ignores =
    __init__.py: F401
exclude =
    airflow
    docs/conf.py
    .git
    __pycache__
    setup.py
    build
    dist
    tests
    .venv
    .tox
    .mypy_cache
    .pytest_cache
    .vscode
    .github
